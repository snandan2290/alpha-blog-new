<h1>Articles index page<h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>description</th>
            <th>action</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do  |article| %>
        <tr>
            <td><%= article.title %></td>
            <td><%= article.description %></td>
            <td>
                <%= link_to 'Delete', article_path(article), method: :delete %>
                <%= link_to 'Show', article_path(article)%>
                <%= link_to 'Edit', edit_article_path(article)%>
            </td>                        
        </tr>
        <% end %>
    </tbody>
</table>